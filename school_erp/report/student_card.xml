<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_student">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h1>Student Report</h1>
                <h2>
                <t t-esc="doc.name"/><br/>
                <t t-esc="doc.last_name"/><br/>
                <t t-esc="doc.student_age"/><br/>
                <t t-esc="doc.gender_male"/><br/>
                <t t-esc="doc.city"/><br/>
                </h2>
            </t>
        </t>
    </template>
</odoo>
